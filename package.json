{
  "name": "eds-site-inspector",
  "version": "1.0.0",
  "description": "EDS Site Inspector - Chrome extension to inspect Edge Delivery Services pages for sections, blocks, and source assets.",
  "type": "commonjs",
  "scripts": {
    "build": "node scripts/build.js",
    "watch": "nodemon --watch src --watch scripts/build.js --exec \"npm run build\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "package": "npm run build && node scripts/package.js",
    "release": "npm run build && node scripts/release.js"
  },
  "keywords": [
    "aem",
    "eds",
    "edge-delivery-services",
    "chrome-extension",
    "inspector"
  ],
  "author": "Hiroyuki Miyata",
  "license": "ISC",
  "devDependencies": {
    "@vitest/ui": "^4.0.14",
    "esbuild": "^0.24.0",
    "js-beautify": "^1.15.4",
    "jsdom": "^27.2.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.7.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "marked": "^17.0.1"
  }
}
